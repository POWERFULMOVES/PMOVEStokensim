{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Ingest Document Ready",
  "type": "object",
  "required": [
    "doc_id",
    "namespace",
    "uri",
    "chunk_count"
  ],
  "properties": {
    "doc_id": {
      "type": "string"
    },
    "namespace": {
      "type": "string"
    },
    "uri": {
      "type": "string"
    },
    "chunk_count": {
      "type": "integer",
      "minimum": 0
    },
    "file_id": {
      "type": "string"
    },
    "title": {
      "type": ["string", "null"]
    },
    "checksum": {
      "type": "string"
    },
    "size_bytes": {
      "type": "integer",
      "minimum": 0
    },
    "preview": {
      "type": "string"
    },
    "meta": {
      "type": "object"
    }
  }
}
