{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Analysis Extract Topics Result",
  "type": "object",
  "required": [
    "media_id",
    "topics"
  ],
  "properties": {
    "media_id": {
      "type": "string"
    },
    "topics": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "label",
          "score"
        ],
        "properties": {
          "label": {
            "type": "string"
          },
          "score": {
            "type": "number",
            "minimum": 0,
            "maximum": 1
          }
        }
      }
    },
    "meta": {
      "type": "object"
    }
  }
}